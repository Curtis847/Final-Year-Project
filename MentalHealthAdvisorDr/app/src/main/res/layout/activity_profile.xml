<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.messenger.practive.mentalhealthadvisordr.ProfileActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/createQuizBtn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/createQuizBtn"
            android:layout_weight="2"
            android:onClick="createQuiz"/>

        <Button
            android:id="@+id/enterLocationBtn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Enter Locationn"
            android:layout_weight="2"
            android:onClick="enterLocation"/>
    </LinearLayout>


</android.support.constraint.ConstraintLayout>
